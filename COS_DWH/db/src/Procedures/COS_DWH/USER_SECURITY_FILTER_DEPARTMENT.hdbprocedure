PROCEDURE "COS_DWH::USER_SECURITY_FILTER_DEPARTMENT" (OUT VALTAB "USER_SECURITY_PRIVILEGE_FILTER_DEP"  ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY DEFINER 
	READS SQL DATA AS
BEGIN
/***************************** 
	Write your procedure logic 
 *****************************/
 
VALTAB = SELECT DISTINCT DEPARTMENT FROM "USER_PRIVILEGE_DEP" WHERE USER_ID = SESSION_USER;
END;