PROCEDURE "COS_DWH::USER_SECURITY_FILTER_BUSINESSUNIT" (OUT VALTAB "USER_SECURITY_PRIVILEGE_FILTER_BU"  ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY DEFINER 
	READS SQL DATA AS
BEGIN
/***************************** 
	Write your procedure logic 
 *****************************/
 
VALTAB = SELECT DISTINCT BUSINESS_UNIT FROM "USER_PRIVILEGE_BU" WHERE USER_ID = SESSION_USER;
END;