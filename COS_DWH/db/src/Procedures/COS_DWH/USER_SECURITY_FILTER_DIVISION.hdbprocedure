PROCEDURE "COS_DWH::USER_SECURITY_FILTER_DIVISION" (OUT VALTAB "USER_SECURITY_PRIVILEGE_FILTER_DIV"  ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY DEFINER 
	READS SQL DATA AS
BEGIN
/***************************** 
	Write your procedure logic 
 *****************************/
 
VALTAB = SELECT DISTINCT DIVISION FROM "USER_PRIVILEGE_DIV" WHERE USER_ID = SESSION_USER;
END;