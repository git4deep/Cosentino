VIEW "V_SCM_MD_MATERIALES" ( "CD_MATERIAL", "DS_MATERIAL_ES", "DS_MATERIAL_EN", "CD_GAMA", "DS_GAMA", "CD_COLOR", "DS_COLOR", "VL_FACTOR", "NMATERIAL", "NTIPO_PRODUCTO" ) AS SELECT
    M.CD_MATERIAL,
    M.DS_MATERIAL_ES,
    M.DS_MATERIAL_EN,
    M.CD_GAMA,
    M.DS_GAMA,
    M.CD_COLOR,
    M.DS_COLOR,
    M.VL_FACTOR,
    M.NMATERIAL,
    M.NTIPO_PRODUCTO
FROM SCM_MD_MATERIALES M
WHERE M.CD_GAMA IN (
    '001',
    '007',
    '010',
    '013',
    '030',
    '037'
)