TRIGGER "SYS_TRIGGER_RUL_DEL_1099073_#0_#" AFTER DELETE ON "SCM_ANALYTIC_SOURCE" REFERENCING OLD ROW OLDROW FOR EACH ROW 
BEGIN  DECLARE KEY_COUNT INT := 0;  DECLARE KEY_COUNT_RR INT := 0;  DECLARE KEY_COUNT_RC INT := 0;  DECLARE foreign_key_err_cond CONDITION FOR SQL_ERROR_CODE 10000;  DECLARE serialize_err_cond CONDITION FOR SQL_ERROR_CODE 10001;  DECLARE EXIT HANDLER FOR foreign_key_err_cond RESIGNAL;  DECLARE EXIT HANDLER FOR serialize_err_cond RESIGNAL; IF CURRENT_TRANSACTION_ISOLATION_LEVEL != 'READ COMMITTED' THEN     SELECT count(*) INTO KEY_COUNT_RR FROM "SCM_HUB_CONTROL" WHERE "ID_SOURC" = :oldrow."ID_SOURC";     SELECT count(*) INTO KEY_COUNT_RC FROM "SCM_HUB_CONTROL" WHERE "ID_SOURC" = :oldrow."ID_SOURC";     IF ( :KEY_COUNT_RR != 0 and :KEY_COUNT_RC = 0 ) or ( :KEY_COUNT_RR = 0 and :KEY_COUNT_RC != 0 )    THEN         SIGNAL serialize_err_cond SET MESSAGE_TEXT = 'serializable failure';     END IF; END IF;  SELECT count(*) INTO KEY_COUNT FROM "SCM_HUB_CONTROL" WHERE "ID_SOURC" = :oldrow."ID_SOURC";  IF :KEY_COUNT != 0  THEN      SIGNAL foreign_key_err_cond SET MESSAGE_TEXT = 'referential constraint violated';  END IF; END;