        <descriptions defaultDescription="SALES_VS_SALARY_MTD"/>
        <formula>if(&quot;SALES_AMOUNT_MTD&quot; != 0,
&quot;COST_ELEMENT_AMOUNT_MTD&quot;/&quot;SALES_AMOUNT_MTD&quot;,
0)*100</formula>
      
