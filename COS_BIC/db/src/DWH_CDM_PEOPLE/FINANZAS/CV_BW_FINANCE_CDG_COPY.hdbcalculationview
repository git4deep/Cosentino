        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DOUBLE" id="SALES_AMOUNT_YTD_" expressionLanguage="SQL">
          <formula>CASE 
when &quot;SALES_CURRENCY_CODE&quot; = 'JPY' then &quot;SALES_AMOUNT_YTD&quot;/100
else &quot;SALES_AMOUNT_YTD&quot;
end</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DOUBLE" id="SALES_AMOUNT_MTD_" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;SALES_CURRENCY_CODE&quot;='JPY',&quot;SALES_AMOUNT_MTD&quot;/100,&quot;SALES_AMOUNT_MTD&quot;)</formula>
